        <filter id="ledGlow" x="-50%" y="-50%" width="200%" height="200%">
          <feGaussianBlur stdDeviation="2" result="blur"/>
          <feMerge><feMergeNode in="blur"/><feMergeNode in="SourceGraphic"/></feMerge>
        </filter>
        <filter id="signalGlow" x="-100%" y="-100%" width="300%" height="300%">
          <feGaussianBlur stdDeviation="4" result="blur"/>
          <feMerge><feMergeNode in="blur"/><feMergeNode in="SourceGraphic"/></feMerge>
        </filter>
        <!-- Ambient light gradients for visibility -->
        <radialGradient id="ambientGlow1" cx="50%" cy="50%" r="50%">
          <stop offset="0%" stop-color="#44d62c" stop-opacity="0.07"/>
          <stop offset="70%" stop-color="#44d62c" stop-opacity="0.02"/>
          <stop offset="100%" stop-color="#44d62c" stop-opacity="0"/>
        </radialGradient>
        <radialGradient id="ambientGlow2" cx="50%" cy="50%" r="50%">
          <stop offset="0%" stop-color="#2ea318" stop-opacity="0.06"/>
          <stop offset="60%" stop-color="#2ea318" stop-opacity="0.015"/>
          <stop offset="100%" stop-color="#2ea318" stop-opacity="0"/>
        </radialGradient>
        <radialGradient id="ambientGlow3" cx="50%" cy="50%" r="50%">
          <stop offset="0%" stop-color="#44d62c" stop-opacity="0.045"/>
          <stop offset="80%" stop-color="#44d62c" stop-opacity="0.01"/>
          <stop offset="100%" stop-color="#44d62c" stop-opacity="0"/>
        </radialGradient>
